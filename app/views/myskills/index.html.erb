<p id="notice"><%= notice %></p>

<h1>Myskills</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @myskills.each do |myskill| %>
      <tr>
        <td><%= myskill.id %></td>
        <td><%= link_to 'Show', myskill %></td>
        <td><%= link_to 'Edit', edit_myskill_path(myskill) %></td>
        <td><%= link_to 'Destroy', myskill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Myskill', new_myskill_path %>
